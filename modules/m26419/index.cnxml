            <colspec colnum="5" colname="c5"/>
            <colspec colnum="6" colname="c6"/>
            <tbody>
              <row>
                <entry>kan</entry>
                <entry>-</entry>
                <entry>can</entry>
                <entry>kan nie</entry>
                <entry>-</entry>
                <entry>cannot</entry>
              </row>
              <row>
                <entry>mag</entry>
                <entry>-</entry>
